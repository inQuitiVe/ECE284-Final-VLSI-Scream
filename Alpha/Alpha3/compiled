TD_huffman_tb.drive_bit ;
    %load/vec4 v0x5630af1d2570_0;
    %store/vec4 v0x5630af1f33b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5630af1f36e0_0, 0, 1;
    %wait E_0x5630af1b9dc0;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5630af1f36e0_0, 0, 1;
    %wait E_0x5630af1b9dc0;
    %end;
